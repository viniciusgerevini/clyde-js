  },
  "module": "./src/index.js",
  "type": "module",
  "exports": {
    ".": {
      "import": "./src/index.js",
      "default": "./cjs/index.js"
    },
    "./package.json": "./package.json"
  },
  "dependencies": {},
  "jest": {
    "transform": {}
